  <xs:complexType name="MedicationKnowledge.Kinetics">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information about a medication that is used to support knowledge.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="BackboneElement">
        <xs:sequence>
