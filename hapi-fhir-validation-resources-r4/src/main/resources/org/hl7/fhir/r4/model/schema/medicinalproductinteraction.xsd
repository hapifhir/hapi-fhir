            </xs:annotation>
            <xs:element name="itemReference" type="Reference"/>
            <xs:element name="itemCodeableConcept" type="CodeableConcept"/>
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
