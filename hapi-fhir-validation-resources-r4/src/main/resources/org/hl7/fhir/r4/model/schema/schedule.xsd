  <xs:include schemaLocation="fhir-base.xsd"/>
  <xs:element name="Schedule" type="Schedule">
    <xs:annotation>
      <xs:documentation xml:lang="en">A container for slots of time that may be available for booking appointments.</xs:documentation>
