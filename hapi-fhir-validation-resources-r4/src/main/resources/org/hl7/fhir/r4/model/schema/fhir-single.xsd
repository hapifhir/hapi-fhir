            </xs:annotation>
           <xs:element name="timingTiming" type="Timing"  maxOccurs="1"/>
           <xs:element name="timingReference" type="Reference"  maxOccurs="1"/>
           <xs:element name="timingDate" type="date"  maxOccurs="1"/>
