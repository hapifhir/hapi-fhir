              <xs:documentation xml:lang="en">A code that represents the type of work a person has held for the longest amount of time during his or her life.</xs:documentation>
           </xs:annotation>
          </xs:element>
          <xs:element name="industry" minOccurs="1" maxOccurs="1" type="CodeableConcept">
            <xs:annotation>
              <xs:documentation xml:lang="en">A code that represents the type of business a person has worked in for the longest total time while in the usual occupation.</xs:documentation>
           </xs:annotation>
          </xs:element>
          <xs:element name="start" minOccurs="0" maxOccurs="1" type="dateTime">
            <xs:annotation>
