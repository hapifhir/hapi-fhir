  <xs:complexType name="OccupationalData.UsualWork">
    <xs:annotation>
      <xs:documentation xml:lang="en">A person's work information, structured to facilitate individual, population, and public health use; not intended to support billing.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="BackboneElement">
        <xs:sequence>
          <xs:element name="occupation" minOccurs="1" maxOccurs="1" type="CodeableConcept">
