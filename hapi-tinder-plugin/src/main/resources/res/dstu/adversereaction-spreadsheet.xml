    <Cell ss:StyleID="s69"/>
    <Cell><Data ss:Type="String">Who had the reaction</Data></Cell>
    <Cell><Data ss:Type="String">The subject of the adverse reaction</Data></Cell>
    <Cell><Data ss:Type="String">intro</Data></Cell>
    <Cell ss:Index="12" ss:StyleID="s69"/>
    <Cell ss:Index="14" ss:StyleID="s69"><Data ss:Type="String">participation[typeCode=SBJ]</Data></Cell>
