            <xs:annotation>
              <xs:documentation xml:lang="en">TheÂ list ofÂ peopleÂ responsible for providing the service.</xs:documentation>
           </xs:annotation>
          </xs:element>
          <xs:element name="appointment" minOccurs="0" maxOccurs="1" type="Reference">
            <xs:annotation>
