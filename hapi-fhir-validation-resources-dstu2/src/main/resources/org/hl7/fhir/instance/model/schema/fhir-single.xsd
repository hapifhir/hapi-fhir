  <xs:complexType name="SearchParameter.Contact">
    <xs:annotation>
      <xs:documentation xml:lang="en">A Search Parameter that defines a named search item that can be used to search/filter on a resource.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
