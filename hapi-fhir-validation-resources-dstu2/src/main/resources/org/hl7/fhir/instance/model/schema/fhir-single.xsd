      <xs:enumeration value="mon">
        <xs:annotation>
          <xs:documentation xml:lang="en">Monday</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tue">
        <xs:annotation>
          <xs:documentation xml:lang="en">Tuesday</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
